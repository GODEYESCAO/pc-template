// 限定文字行数后省略
.line1{ 
	// overflow : hidden;
	// text-overflow: ellipsis;
	// display: -webkit-box;
	// -webkit-line-clamp: 1; 
	// -webkit-box-orient: vertical;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  &:after{
    content:"";
    font-weight:bold;
    position:absolute;
    bottom:0;
    right:0;
    padding:0 20px 1px 0px;///控制后面省略号的位置
  }
}
.line2{ 
	// text-overflow: ellipsis;
	// display: -webkit-box;
	// -webkit-line-clamp: 2;
	// -webkit-box-orient: vertical;
	// overflow:hidden;
  // white-space:nowrap;
  overflow:hidden; 
  text-overflow:ellipsis;
  display:-webkit-box; 
  -webkit-box-orient:vertical;
  -webkit-line-clamp:2; 
  &:after{
    content:"";
    font-weight:bold;
    position:absolute;
    bottom:0;
    right:0;
    padding:0 20px 1px 0px;///控制后面省略号的位置
  }
}
.line3{ 
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-line-clamp: 3;
	-webkit-box-orient: vertical;
	overflow : hidden;
  &:after{
    content:"";
    font-weight:bold;
    position:absolute;
    bottom:0;
    right:0;
    padding:0 20px 1px 0px;///控制后面省略号的位置
  }
}
